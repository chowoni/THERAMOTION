        <Image Margin="73,650,73,0" Source="/res/m3/lBG.png" Stretch="Fill" Height="94" VerticalAlignment="Top"/>

        <Image Margin="111,667,0,0" Source="/res/m3/ic_ok_48.png" Stretch="Fill" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="58" RenderTransformOrigin="0.362,0.517"/>

