            </RadioButton>
            <RadioButton x:Name="btn2" RenderTransformOrigin="0.467,0.518" Click="QnA" VerticalAlignment="Top" Margin="1,175,0,0" HorizontalAlignment="Left" BorderBrush="{x:Null}" BorderThickness="0" Background="{x:Null}">
