<Application x:Class="ai.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ai"
             StartupUri="Page8_1.xaml">
    <Application.Resources>
        <FontFamily x:Key="Pretendard">pack://application:,,,/font/#Pretendard</FontFamily>
        <FontFamily x:Key="Pretendard-Black">pack://application:,,,/font/#Pretendard Black</FontFamily>
        <FontFamily x:Key="Pretendard-ExtraBlack">pack://application:,,,/font/#Pretendard ExtraBold</FontFamily>
        <FontFamily x:Key="Pretendard-ExtraLight">pack://application:,,,/font/#Pretendard ExtraLight</FontFamily>
        <FontFamily x:Key="Pretendard-Light">pack://application:,,,/font/#Pretendard Light</FontFamily>
        <FontFamily x:Key="Pretendard-Medium">pack://application:,,,/font/#Pretendard Medium</FontFamily>
        <FontFamily x:Key="Pretendard-SemiBold">pack://application:,,,/font/#Pretendard SemiBold</FontFamily>
    </Application.Resources>
</Application>
