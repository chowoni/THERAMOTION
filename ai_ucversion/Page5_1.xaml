            <RadioButton x:Name="btn1" RenderTransformOrigin="0.467,0.518" Height="14" Click="QnA" VerticalAlignment="Top" Margin="1,88,0,0" HorizontalAlignment="Left" BorderBrush="{x:Null}" BorderThickness="0" Width="12" Background="{x:Null}">
